   "source": [
    "def first_Unique_char(S):\n",
    "    ans=-1\n",
    "    for i in range(len(S)-1):\n",
    "        strg=S[0:i]+S[i+1:]\n",
    "        if(strg.find(S[i])==-1):\n",
    "            return i\n",
    "    return ans"
   ]
